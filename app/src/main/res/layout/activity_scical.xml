<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.faizanali.allinone.Scical"
    android:background="@drawable/a9">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Scintific Calculator"
        android:id="@+id/textView9"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:textSize="35dp"
        android:textStyle="bold"
        android:typeface="serif"
        android:textColor="#ffffff" />

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/imageView6"
        android:layout_below="@+id/textView9"
        android:layout_alignParentStart="true"
        android:src="@drawable/l5" />

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/imageView7"
        android:src="@drawable/l4"
        android:layout_alignTop="@+id/imageView6"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Input  "
        android:id="@+id/textView10"

        android:textSize="25dp"
        android:textStyle="bold"
        android:textColor="#ffffff"
        android:layout_alignTop="@+id/editText10"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/editText10"
        android:textSize="25dp"
        android:layout_marginTop="30dp"
        android:textStyle="bold"
        android:textColor="#ff0101"
        android:background="#ffffff"
        android:layout_above="@+id/textView11"
        android:layout_alignStart="@+id/textView11" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="sin"
        android:id="@+id/button23"
        android:textStyle="bold"
        android:typeface="serif"
        android:background="@drawable/rounded_corners"
        android:textColor="#f91e1e"
        android:layout_below="@+id/textView12"
        android:layout_alignParentStart="true"
        android:layout_alignEnd="@+id/button25" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="cos"
        android:id="@+id/button24"
        android:layout_alignTop="@+id/button23"
        android:layout_alignEnd="@+id/imageView7"
        android:typeface="serif"
        android:textStyle="bold"
        android:background="@drawable/rounded_corners"
        android:textColor="#f91e1e" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="tan"
        android:id="@+id/button25"
        android:layout_below="@+id/button23"
        android:layout_alignParentStart="true"
        android:layout_marginTop="40dp"
        android:layout_alignEnd="@+id/imageView6"
        android:textStyle="bold"
        android:typeface="serif"
        android:background="@drawable/rounded_corners"
        android:textColor="#f91e1e" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="cot"
        android:id="@+id/button26"
        android:layout_alignTop="@+id/button25"
        android:layout_alignStart="@+id/button24"
        android:layout_alignEnd="@+id/imageView7"
        android:textStyle="bold"
        android:typeface="serif"
        android:background="@drawable/rounded_corners"
        android:textColor="#f91e1e" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="sec"
        android:id="@+id/button27"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_alignEnd="@+id/button25"
        android:textStyle="bold"
        android:typeface="serif"
        android:background="@drawable/rounded_corners"
        android:textColor="#f91e1e" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="cosec"
        android:id="@+id/button28"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/button26"
        android:layout_alignParentEnd="true"
        android:textStyle="bold"
        android:typeface="serif"
        android:background="@drawable/rounded_corners"
        android:textColor="#f91e1e" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Result  "
        android:id="@+id/textView12"
        android:textStyle="bold"
        android:textSize="25dp"

        android:layout_marginBottom="30dp"
        android:textColor="#ffffff"
        android:layout_below="@+id/textView10"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView11"
        android:layout_centerVertical="true"
        android:textSize="25dp"

        android:textStyle="bold"
        android:textColor="#fefefe"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/button23"
        android:background="#ff0101" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="back"
        android:id="@+id/button31"
        android:textStyle="bold"
        android:typeface="serif"
        android:background="@drawable/rounded_corners"
        android:textColor="#f91e1e"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

</RelativeLayout>
