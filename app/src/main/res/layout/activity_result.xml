<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.faizanali.allinone.Result"
    android:background="@drawable/a12">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView21"
        android:layout_marginTop="54dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:textSize="20dp"
        android:textStyle="bold"
        android:typeface="serif"
        android:textColor="#ffffff"
        android:background="#000000" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="              Result"
        android:id="@+id/textView22"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:textSize="35dp"
        android:textStyle="bold"
        android:typeface="serif"
        android:textColor="#ffffff" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="facebook"
        android:id="@+id/button49"
        android:layout_below="@+id/textView21"
        android:layout_alignParentStart="true"
        android:layout_marginTop="69dp"
        android:textColor="#ff0101"
        android:textStyle="bold"
        android:typeface="serif"
        android:background="@drawable/rounded_corners" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Instagram"
        android:id="@+id/button50"
        android:layout_marginTop="30dp"
        android:layout_below="@+id/button49"
        android:layout_alignParentStart="true"
        android:textColor="#ff0101"
        android:textStyle="bold"
        android:typeface="serif"
        android:background="@drawable/rounded_corners" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Netcamp.in"
        android:id="@+id/button51"
        android:layout_marginTop="30dp"
        android:layout_below="@+id/button50"
        android:layout_alignParentStart="true"
        android:textColor="#ff0101"
        android:textStyle="bold"
        android:typeface="serif"
        android:background="@drawable/rounded_corners" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="home"
        android:id="@+id/button52"

        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/button51"
        android:layout_alignParentStart="true"
        android:textColor="#ff0101"
        android:textStyle="bold"
        android:typeface="serif"
        android:background="@drawable/rounded_corners" />
</RelativeLayout>
